entropy <- function(left,right) {
total <- abs(left) + abs(right)
if (abs(left) != 0)
p_left <- left/total
else
p_left <- 0
if (abs(right) != 0)
p_right <- right/total
else
p_right <- 0
E <- -p_left * log(p_left,base = 2) - p_right * log(p_right,base=2)
return(E)
}
